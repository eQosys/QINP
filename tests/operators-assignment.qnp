import "stdio.qnp"

fn<> show(i32 variable)...

var<i32> i = 1
show(i)

show(i += 3)

show(i -= 2)

show(i *= 101)

show(i /= 2)

show(i %= 6)

show(i &= 4)

show(i |= 16)

show(i ^= 4)

show(i <<= 1)

show(i >>= 2)

fn<> show(i32 variable)!:
	std.print("var = ")
	std.print(variable)
	std.print("\n")