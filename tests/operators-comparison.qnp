import "stdio.qnp"

fn<> show(bool b):
	std.print("b = ")
	if b:
		std.print("true")
	else:
		std.print("false")
	std.print("\n")

show(1 == 2)
show(1 == 1)
show(0 == 0)

show(1 != 2)
show(1 != 1)
show(0 != 0)

show(1 < 2)
show(1 < 1)
show(2 < 1)

show(1 > 2)
show(1 > 1)
show(2 > 1)

show(1 <= 2)
show(1 <= 1)
show(2 <= 1)

show(1 >= 2)
show(1 >= 1)
show(2 >= 1)